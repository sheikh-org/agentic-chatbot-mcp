{
  "name": "agentic-chatbot-mcp",
  "version": "1.0.0",
  "private": true,
  "description": "Agentic chatbot with Chrome DevTools MCP integration for browser automation",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json server.js",
    "dev:next": "next dev",
    "dev:ws": "ts-node --project tsconfig.server.json src/utils/vnc-websocket-server.ts",
    "dev:mcp": "npx chrome-devtools-mcp@latest --headless --viewport 1920x1080",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:mcp\"",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "install:all": "npm install && npm run install:ai-sdk",
    "install:ai-sdk": "npm install @ai-sdk/google ai",
    "setup:mcp": "echo '{\"mcpServers\": {\"chrome-devtools\": {\"command\": \"npx\", \"args\": [\"chrome-devtools-mcp@latest\"]}}}' > mcp-config.json",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:install": "npx playwright install",
    "test:install:deps": "npx playwright install-deps",
    "clean": "rm -rf .next dist node_modules/.cache",
    "deploy": "npm run build && npm run start"
  },
  "dependencies": {
    "@ai-sdk/google": "^0.0.0",
    "@google/generative-ai": "^0.7.0",
    "@types/ws": "^8.5.0",
    "ai": "^3.0.0",
    "autoprefixer": "^10.4.0",
    "canvas": "^2.11.0",
    "framer-motion": "^10.16.0",
    "lucide-react": "^0.292.0",
    "next": "^14.0.0",
    "openai": "^4.20.0",
    "postcss": "^8.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "tailwindcss": "^3.3.0",
    "uuid": "^9.0.0",
    "ws": "^8.14.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "ai",
    "chatbot",
    "mcp",
    "browser-automation",
    "chrome-devtools",
    "agentic",
    "google-generative-ai",
    "real-time",
    "web-automation"
  ],
  "author": "MiniMax Agent",
  "license": "MIT"
}